#!/usr/bin/python3
def fizzbuzz():
    for i in range(100):
        if (i + 1) % 3 == 0 and (i + 1) % 5 == 0:
            print("fizzbuzz ", end='')
        elif (i + 1) % 3 == 0:
            print("fizz ", end='')
        elif (i + 1) % 5 == 0:
            print("buzz ", end='')
        else:
            print(i + 1, end='')
